{
  "expo": {
    "sdkVersion": "43.0.0",
    "name": "Vegeta",
    "slug": "app_vegeta",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CDD8D9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "<android maps key>"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pi2ucl.vegeta",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDOrCSFGZ2acJ9G-kEiur4LfBQSzTT_XIQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
